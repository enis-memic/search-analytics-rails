<h1>Search Query Analytics</h1>

<table>
  <thead>
    <tr>
      <th>Search Query</th>
      <th>View Count</th>
    </tr>
  </thead>
  <tbody>
    <% @user_queries.each do |query| %>
      <tr>
        <td><%= query.search_query %></td>
        <td><%= Query.views_count(query.search_query, request.remote_ip) %></td>
      </tr>
    <% end %>
  </tbody>
</table>